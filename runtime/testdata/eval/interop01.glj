(def regexp (regexp.MustCompile "foo(.*)bar"))
(print (. regexp (Match ((go-sliceof go/byte) "foo----bar"))))
(print (. regexp (FindStringSubmatch "foo----bar")))

(print (regexp.Match "foo.*bar" nil))
(print (regexp.Match "foo.*bar" ((go-sliceof go/byte) "foobar")))
(print (regexp.Match "foo(.*)bar" ((go-sliceof go/byte) "foo----bar")))
